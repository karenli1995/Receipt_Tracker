<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.karenli.budgetingapp.ViewReceiptsActivity">

    <include layout="@layout/toolbar_app" />

    <ListView
        android:id="@+id/lvReceipts"
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp" />

    <FrameLayout
        android:id="@+id/chosenReceiptsFragment"
        class="com.example.karenli.budgetingapp.ChosenReceiptFragment"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:orientation="horizontal"></FrameLayout>


</RelativeLayout>
